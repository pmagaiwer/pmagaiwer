{
  "widgets": [
    {
      "type": "metric",
      "x": 0,
      "y": 0,
      "width": 12,
      "height": 6,
      "properties": {
        "title": "CPU Utilization",
        "metrics": [
          [ "AWS/RDS", "CPUUtilization", "DBInstanceIdentifier", "MEU_RDS" ]
        ],
        "region": "us-east-1",
        "stat": "Average",
        "period": 300
      }
    },
    {
      "type": "metric",
      "x": 12,
      "y": 0,
      "width": 12,
      "height": 6,
      "properties": {
        "title": "Database Connections",
        "metrics": [
          [ "AWS/RDS", "DatabaseConnections", "DBInstanceIdentifier", "MEU_RDS" ]
        ],
        "region": "us-east-1",
        "stat": "Average",
        "period": 300
      }
    },
    {
      "type": "metric",
      "x": 0,
      "y": 6,
      "width": 12,
      "height": 6,
      "properties": {
        "title": "Read / Write Latency",
        "metrics": [
          [ "AWS/RDS", "ReadLatency", "DBInstanceIdentifier", "MEU_RDS" ],
          [ ".", "WriteLatency", ".", "." ]
        ],
        "region": "us-east-1",
        "period": 300
      }
    },
    {
      "type": "metric",
      "x": 12,
      "y": 6,
      "width": 12,
      "height": 6,
      "properties": {
        "title": "Free Storage Space",
        "metrics": [
          [ "AWS/RDS", "FreeStorageSpace", "DBInstanceIdentifier", "MEU_RDS" ]
        ],
        "region": "us-east-1",
        "stat": "Minimum",
        "period": 300
      }
    }
  ]
}
